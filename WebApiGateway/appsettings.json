{
  "ReverseProxy": {
    "Routes": {
      "user": {
        "ClusterId": "webapi",
        "Match": {
          "Path": "/api/user/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "login": {
        "ClusterId": "webapi",
        "Match": {
          "Path": "/api/login/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      }
    },
    "Clusters": {
      "myappapi": {
        "Destinations": {
          "myappapi_dest": {
            "Address": "https://localhost:64143/"
          }
        }
      }
    }
  }
}
